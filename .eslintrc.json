{
  "extends": [
    "next/core-web-vitals",
    "prettier",
    "eslint:recommended"
  ],
  "rules": {
    "indent": [
      "error",
      2
    ],
    "no-unreachable": "off",
    "quotes": [
      "error",
      "single"
    ],
    "jsx-quotes": [
      "error",
      "prefer-single"
    ],
    "semi": [
      "error",
      "never"
    ]
  }
}
